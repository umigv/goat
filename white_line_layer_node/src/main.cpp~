#include "WhiteLineDetection.h"
#include "ros/ros.h"


int main(int argc, char ** argv)
{
  DetectWhiteLines detecter;
  ros::init(argc,argv,"whiteLineDetect");
  ros::NodeHandle n;
  // ros::Subscriber sub = n.subscribe("/imu/data",1000,detecter());
  ros::Subscriber sub = n.subscribe("chatter",1000,detecter);
  ros::spin();
  
return  0;
}
